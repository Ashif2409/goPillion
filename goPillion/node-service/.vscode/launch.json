{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug @node-service/auth-service with Nx",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "@node-service/auth-service"],
      "env": {
        "NODE_OPTIONS": "--inspect=9229"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/auth-service/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
